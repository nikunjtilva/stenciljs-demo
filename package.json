  "license": "MIT",
  "devDependencies": {
    "@stencil/core": "^0.8.0",
    "@stencil/dev-server": "0.0.18",
    "@stencil/sass": "0.0.3",
    "@stencil/utils": "0.0.4",
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
